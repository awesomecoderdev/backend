{
    "name": "laravel/laravel",
    "type": "project",
    "description": "The Laravel Framework.",
    "keywords": [
        "framework",
        "laravel"
    ],
    "license": "MIT",
    "require": {
        "php": "^8.0.2",
        "guzzlehttp/guzzle": "^7.2",
        "laravel/framework": "^9.19",
        "laravel/sanctum": "^3.0",
        "laravel/socialite": "^5.5",
        "laravel/tinker": "^2.7",
        "laravelcollective/html": "^6.3",
        "mpdf/mpdf": "^8.1",
        "psr/log": "^2.0",
        "stripe/stripe-php": "^10.1",
        "ext-json": "*",
        "illuminate/console": "^9.21|^10.0",
        "illuminate/contracts": "^9.21|^10.0",
        "illuminate/database": "^9.21|^10.0",
        "illuminate/http": "^9.21|^10.0",
        "illuminate/log": "^9.21|^10.0",
        "illuminate/notifications": "^9.21|^10.0",
        "illuminate/pagination": "^9.21|^10.0",
        "illuminate/routing": "^9.21|^10.0",
        "illuminate/support": "^9.21|^10.0",
        "illuminate/view": "^9.21|^10.0",
        "moneyphp/money": "^4.0",
        "nesbot/carbon": "^2.0",
        "symfony/http-kernel": "^6.0",
        "symfony/polyfill-intl-icu": "^1.22.1"
    },
    "require-dev": {
        "fakerphp/faker": "^1.9.1",
        "laravel/pint": "^1.0",
        "laravel/sail": "^1.0.1",
        "mockery/mockery": "^1.4.4",
        "nunomaduro/collision": "^6.1",
        "phpunit/phpunit": "^9.5.10",
        "spatie/laravel-ignition": "^1.0"
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/",
            "Database\\Factories\\": "database/factories/",
            "Database\\Seeders\\": "database/seeders/",
            "AwesomeCoder\\ShoppingCart\\": "cart/",
            "AwesomeCoder\\": "core/",
            "Laravel\\Cashier\\": "packages/cashier/src/",
            "Laravel\\Cashier\\Database\\Factories\\": "packages/cashier/factories/"
        },
        "files": [
            "core/Helpers.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/",
            "Laravel\\Cashier\\Tests\\": "packages/cashier/tests/"
        }
    },
    "scripts": {
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover --ansi"
        ],
        "post-update-cmd": [
            "@php artisan vendor:publish --tag=laravel-assets --ansi --force"
        ],
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate --ansi"
        ]
    },
    "extra": {
        "branch-alias": {
            "dev-master": "14.x-dev"
        },
        "laravel": {
            "dont-discover": [],
            "providers": [
                "Laravel\\Cashier\\CashierServiceProvider"
            ]
        }
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true,
        "allow-plugins": {
            "pestphp/pest-plugin": true
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
